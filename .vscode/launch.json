    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "a.out",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
        "name": "C++ Launch (Windows)",
        "type": "cppvsdbg",
        "request": "launch",
        "program": "${fileDirname}\\${fileBasenameNoExtension}.exe",
        "symbolSearchPath": "C:\\Symbols;C:\\SymbolDir2",
        "cwd": "${workspaceRoot}",
        "logging": {
            "moduleLoad": false,
            "trace": true
        },
        "visualizerFile": "${workspaceFolder}/my.natvis",
        "internalConsoleOptions": "openOnSessionStart",
    },
    {
         "name": "(gdb) Launch",
         "type": "cppdbg",
         "request": "launch",
        "program": "${fileDirname}\\${fileBasenameNoExtension}.exe",
         "args": [],
         "stopAtEntry": false,
         "cwd": "${workspaceFolder}",
         "environment": [],
         "externalConsole": false,
         "MIMode": "gdb",
        "preLaunchTask": "C/C++: g++.exe build active file",
        "miDebuggerPath": "C:/MinGW/mingw64/bin/gdb.exe",
         "setupCommands": [
             {
                 "description": "Enable pretty-printing for gdb",
                 "text": "-enable-pretty-printing",
                 "ignoreFailures": true
             }
         ]
     },



    ]
}